{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-angular",
        "output": "./api",
        "removeOperationIdPrefix": true,
        "inputSpec": "http://localhost:5033/swagger/v1/swagger.json",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": "true",
          "stringEnums": "true",
          "enablePostProcessFile": "true",
          "removeOperationIdPrefix": "true",
          "serviceSuffix": "ServiceGen"
        },
        "minimalUpdate": false
      }
    }
  }
}
